			update_time = #{updateDateTime},
			update_user_id = #{updateUserId},
			delete_flag = #{deleteFlag}
		WHERE
			id = #{id}
	</update>
	
	<insert id="insert" parameterType="com.djb.domain.qa.QuestionClassify">

		  from	Dp_Tender_Dispatch_Record
		 where	delete_flag = 0
		   and	agent_id = #{agentId}
		   and	Type = #{type}
		   and	create_time &gt;= #{weekStart}
		   and	Dispatch_Type = 2
	</select>
	

		INSERT INTO 
			dq_question_classify(
				question_id,
				classify_id,
				create_time,
				create_user_id,
				update_time,
				update_user_id,
				delete_flag
			)
		VALUES(
			#{question.id},
			#{insuranceClassify.id},
			#{createDateTime},

				type,
				answer_user_name,
				answer_user_photo,
				create_user_id, 
				create_time, 
				update_user_id, 
				update_time,
				delete_flag,
				admin_answer
			)
		VALUES(
			#{uuid}, 
			#{question.uuid}, 
			#{content}, 
			#{type}, 
			#{answerUserName},
			#{answerUserPhoto},
			#{createUserId},
			#{createDateTime},
			#{updateUserId},
			#{updateDateTime},
			#{deleteFlag},
			#{isAdminAnswer}
		)

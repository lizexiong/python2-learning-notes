		   AND	Consume_Time >= #{startTime}
		 </if>
		 <if test="null != endTime">
		   AND	Consume_Time &lt;= #{endTime}
		 </if>
		 </if>
		 <if test="2 == type">
		 <if test="null != startTime">
		   AND	Reg_Time >= #{startTime}
		 </if>
		 <if test="null != endTime">
		   AND	Reg_Time &lt;= #{endTime}
		 </if>

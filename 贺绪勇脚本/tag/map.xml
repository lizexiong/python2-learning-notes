					,Pay_Dates
					,Package_Type
					,Contract_Open_Status
					,Contract_Open_Type
					,Contract_Open_Datetime
					,Contract_Expired_Status
					,Contract_Expired_Datetime
					,Contract_Id
					,Create_Time
					,Create_User_Id
					,Update_Time
					,Update_User_Id
					,Delete_Flag
				)
			VALUES(
				#{agentId}
				,#{agentName}
				,#{agentProvinceName}
				,#{agentCityName}
				,#{agentCompanyId}
				,#{agentCompanyName}

			 				 <foreach collection="questionIds" item="item" separator="," close=")" open="(">
	     					#{item}
	    				</foreach>
	    </if>
	ORDER BY	ANSWER.create_time DESC
	</select>
	
	<insert id="insert" parameterType="com.djb.domain.qa.Answer">
